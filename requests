# requests

#crear una nota

curl -X POST -H "Content-Type: application/json" -d '{"text": "algo"}' localhost:3000/api/notes

#borrar una nota


curl -x DELETE localhost/3000/api/notes/6177103e9a7c486c2b4fcdc4


#modificar una nota


curl -X PUT -H "Content-Type: application/json" -d '{"title": "PAPU", "text": "algo"}'  localhost:3000/api/notes
